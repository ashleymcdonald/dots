{
   "background": {
      "persistent": false,
      "scripts": [ "js/background.min.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon_48.png"
   },
   "content_scripts": [ {
      "js": [ "js/content.min.js" ],
      "matches": [ "http://*/*", "https://*/*", "file://*/*" ]
   } ],
   "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://fonts.googleapis.com https://www.google-analytics.com; object-src 'self'",
   "description": "Take control of your tabs",
   "homepage_url": "https://github.com/sylouuu/chrome-tab-modifier",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Tab Modifier",
   "options_page": "html/options.min.html",
   "permissions": [ "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.14.3",
   "web_accessible_resources": [ "img/*.png" ]
}
